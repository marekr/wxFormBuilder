<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="wxAdditions Plugin" />
		<Option pch_mode="0" />
		<Option default_target="Debug (Unicode)" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\..\bin\plugins\wxAdditions\libwxadditionsd.dll" />
				<Option object_output=".objsd" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG__" />
					<Add directory="$(#WX.lib)\gcc_dll\mswd" />
				</Compiler>
				<Linker>
					<Add library="tinyxmld" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="d" />
					<Variable name="WXADD_SUFFIX" value="md" />
				</Environment>
			</Target>
			<Target title="Debug (Unicode)">
				<Option output="..\..\..\bin\plugins\wxAdditions\libwxadditionsd.dll" />
				<Option object_output=".objsud" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="..\..\..\bin\wxFormBuilderd.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG__" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DUNICODE" />
				</Compiler>
				<Linker>
					<Add library="tinyxmld" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="ud" />
					<Variable name="WXADD_SUFFIX" value="umd" />
				</Environment>
			</Target>
			<Target title="Release">
				<Option output="..\..\..\bin\plugins\wxAdditions\libwxadditions.dll" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="tinyxml" />
				</Linker>
			</Target>
			<Target title="Release (Unicode)">
				<Option output="..\..\..\bin\plugins\wxAdditions\libwxadditions.dll" />
				<Option object_output=".objsu" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DUNICODE" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="tinyxml" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WXADD_SUFFIX" value="um" />
				</Environment>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="" />
				<Variable name="WX_VER" value="270" />
				<Variable name="tinyxml" value="..\..\..\sdk\tinyxml" />
				<Variable name="plugin_interface" value="..\..\..\sdk\plugin_interface" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-DBUILD_DLL" />
			<Add option="-D__WX__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DTIXML_USE_TICPP" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#wxadditions.include)" />
			<Add directory="$(tinyxml)" />
			<Add directory="..\.." />
			<Add directory="$(plugin_interface)" />
		</Compiler>
		<Linker>
			<Add library="wxmsw$(WX_VER)$(WX_SUFFIX)_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_plotctrl_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_things_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_awx_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_propgrid_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_scintilla_gcc" />
			<Add library="wxmsw$(WX_VER)$(WXADD_SUFFIX)_flatnotebook_gcc" />
			<Add directory="$(#WX.lib)\gcc_dll" />
			<Add directory="$(#wxadditions.lib)\gcc_dll" />
			<Add directory="$(tinyxml)\lib" />
			<Add directory="$(plugin_interface)\lib" />
		</Linker>
		<Unit filename="wxAdditions.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Debug (Unicode)" />
			<Option target="Release" />
			<Option target="Release (Unicode)" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>